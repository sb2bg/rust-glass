square = func(x) => {
    return x * x + [x * x];
}

square(2);

offset_by_seven_point_two = func(x) => {
    return x + 7.2;
}

test = offset_by_seven_point_two(2);
println("2 + 7.2 = " + test == 9.2);

create_range = func(start, end) => {
    return start..end;
}

println(create_range(1, 10));
print(create_range(1, 10).length);
println(" <- length of range");

